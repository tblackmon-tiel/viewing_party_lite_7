<div id="homepage-buttons">
  <% if session[:user_id] %>
    <%= link_to "Log Out", logout_path %>
    <br><br>
    <div id="all-users">
      <h3>Existing Users</h3>
      <% @users.each do |user| %>
        <p><%= user.email %></p>
      <% end %>
    </div>
  <% else %>
    <p><%= link_to "Create an Account", register_path %></p>
    <p><%= link_to "Log In", login_path %></p>
  <% end %>
</div> 